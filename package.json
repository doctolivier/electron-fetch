{
  "name": "electron-fetch",
  "version": "2.0.0",
  "description": "A light-weight module that brings window.fetch to electron's background process",
  "type": "module",
  "files": [
    "./lib"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      },
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      }
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsx build.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arantes555/electron-fetch.git"
  },
  "keywords": [
    "fetch",
    "http",
    "promise",
    "electron"
  ],
  "author": "Mehdi Kouhen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arantes555/electron-fetch/issues"
  },
  "homepage": "https://github.com/arantes555/electron-fetch",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "abortcontroller-polyfill": "^1.7.8",
    "electron": "^33",
    "esbuild": "^0.24.0",
    "form-data": "^4.0.1",
    "parted": "^0.1.1",
    "proxy": "^2.2.0",
    "resumer": "0.0.0",
    "stoppable": "^1.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "zx": "^8.2.4"
  },
  "dependencies": {
    "encoding": "^0.1.13",
    "iconv-lite": "^0.6.3"
  }
}
